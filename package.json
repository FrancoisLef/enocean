{
  "name": "enocean",
  "version": "0.0.0",
  "description": "A new CLI generated with oclif",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/FrancoisLef/enocean",
  "bugs": "https://github.com/FrancoisLef/enocean/issues",
  "repository": "FrancoisLef/enocean",
  "license": "MIT",
  "author": "FranÃ§ois Lefebvre",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "enocean": "./bin/run.js"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "format": "prettier --write .",
    "lint": "eslint",
    "prepack": "oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "test": "mocha --forbid-only \"test/**/*.test.ts\" \"src/**/*.test.ts\"",
    "posttest": "npm run lint",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "enocean",
    "commands": "./dist/commands",
    "dirname": "enocean",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "dongle": {
        "description": "Manage EnOcean dongle"
      }
    }
  },
  "dependencies": {
    "@inquirer/prompts": "7.5.3",
    "@oclif/core": "4.4.0",
    "@oclif/plugin-help": "6.2.29",
    "chalk": "5.4.1",
    "serialport": "13.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "1.3.0",
    "@oclif/test": "4.1.13",
    "@types/chai": "5.2.2",
    "@types/mocha": "10.0.10",
    "@types/node": "24.0.3",
    "@types/sinon": "^17.0.4",
    "chai": "5.2.0",
    "eslint": "9.29.0",
    "eslint-config-oclif": "6.0.72",
    "eslint-config-prettier": "10.1.5",
    "mocha": "11.7.0",
    "oclif": "4.19.0",
    "prettier": "3.5.3",
    "prettier-plugin-packagejson": "2.5.15",
    "shx": "0.4.0",
    "sinon": "^21.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
